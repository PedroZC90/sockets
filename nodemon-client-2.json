{
    "verbose": true,
    "watch": [
        "./src"
    ],
    "ext": "ts, env",
    "ignore": [
        "node_modules/**/*",
        "dist/*",
        "docs/*"
    ],
    "delay": "500",
    "env": {
        "NAME": "node-02"
    },
    "execMap": {
        "ts": "node --inspect=9102 -r ts-node/register"
    }
}
